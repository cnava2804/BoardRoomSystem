@model BoardRoomSystem.Models.Reservations

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Delete</h1>

<h3>¿Estas seguro de eliminar esta reservación?</h3>
<div>
   
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Reservation_Subject)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Reservation_Subject)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Reservation_Recipient)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Reservation_Recipient)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Reservation_StartDate)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Reservation_StartDate)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Reservation_EndtDate)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Reservation_EndtDate)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Reservation_NumbPeople)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Reservation_NumbPeople)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Reservation_Description)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Reservation_Description)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Reservation_Delegate)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Reservation_Delegate)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Reservation_Id" />
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a class="btn btn-warning" asp-action="Index">Regresar</a>
    </form>
</div>
